{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/gsi/route.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/style.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/scoreboard",
      "dest": "src/app/api/gsi/route.js"
    },
    {
      "src": "/api/gsi",
      "dest": "src/app/api/gsi/route.js"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
